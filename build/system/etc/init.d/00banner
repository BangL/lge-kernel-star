#!/system/bin/sh
#
# Print startup info
#
L="log -p i -t cm"

$L "Welcome to Android `getprop ro.build.version.release` / CyanogenMod-`getprop ro.cm.version`";
$L "                                                _ ";
$L " __ __ _ ___  _ _  __  ___  __ _ _  _  _  __  __))";
$L "((_ \\(/'((_( ((\\( ((_)((_( (('((\\( ((\`1( ((_)((_( ";
$L "     ))                 _))";
$L " ";


# ksm
echo 1500 > /sys/kernel/mm/ksm/sleep_millisecs
echo 256 > /sys/kernel/mm/ksm/pages_to_scan
echo 1 > /sys/kernel/mm/ksm/run
